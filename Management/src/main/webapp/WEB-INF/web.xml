<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveStudent</servlet-name>
  	<display-name>SaveStudent</display-name>
  	<description></description>
  	<servlet-class>com.servlet.SaveStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>delete</servlet-name>
  	<display-name>delete</display-name>
  	<description></description>
  	<servlet-class>com.servlet.delete</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateStudent</servlet-name>
  	<display-name>UpdateStudent</display-name>
  	<description></description>
  	<servlet-class>com.servlet.UpdateStudent</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>SaveStudent</servlet-name>
  	<url-pattern>/SaveStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateStudent</servlet-name>
  	<url-pattern>/UpdateStudent</url-pattern>
  </servlet-mapping>
 


</web-app>
